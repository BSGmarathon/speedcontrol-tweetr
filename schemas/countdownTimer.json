{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"countdownActive": {
			"type": "boolean",
			"default": false
		},
		"cancelTweet": {
			"type": "boolean",
			"default": false
		},
		"sendTweet": {
			"type": "boolean",
			"default": false
		},
		"countdown": {
			"type": "integer",
			"default": -1
		}
	},
	"required": [
		"countdownActive",
		"cancelTweet",
		"sendTweet",
		"countdown"
	]
}
