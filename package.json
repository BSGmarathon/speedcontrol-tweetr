{
  "name": "speedcontrol-tweetr",
  "version": "1.2.1",
  "description": "Control Twitter right from your NodeCG dashboard!",
  "homepage": "https://github.com/nicnacnic/speedcontrol-tweetr",
  "author": "nicnacnic",
  "url": "https://nicnacnic.com",
  "license": "MIT",
  "nodecg": {
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.4.0",
      "nodecg-components": "^1.0.0"
    },
    "compatibleRange": "^1.8.1",
    "assetCategories": [
      {
        "name": "media",
        "title": "Tweet Media",
        "allowedTypes": [
          "png",
          "jpg",
          "gif",
          "jpeg",
          "webp",
          "mp4"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "tweetr",
        "title": "Tweetr",
        "file": "tweetr/tweetr.html",
        "width": 3
      },
      {
        "name": "editTweet",
        "title": "",
        "width": 4,
        "file": "editTweet/editTweet.html",
        "dialog": true
      },
      {
        "name": "settings",
        "title": "Tweetr Settings",
        "workspace": "Settings",
        "file": "settings/settings.html",
        "width": 3
      }
    ]
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.6",
    "twitter-api-client": "^1.3.7",
    "twitter-api-v2": "^1.12.7"
  }
}
