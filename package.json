{
  "name": "speedcontrol-tweetr",
  "version": "1.2.0",
  "description": "Control Twitter right from your NodeCG dashboard!",
  "homepage": "https://github.com/nicnacnic/speedcontrol-tweetr",
  "author": "nicnacnic",
  "url": "https://nicnacnic.com",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config"
  },
  "nodecg": {
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.4.0"
    },
    "compatibleRange": "^1.8.1",
    "assetCategories": [
      {
        "name": "media",
        "title": "Tweet Media",
        "allowedTypes": [
          "png",
          "jpg",
          "gif",
          "mp4"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "tweetr",
        "title": "Tweetr",
        "file": "tweetr.html",
        "width": 3
      },
      {
        "name": "edit-tweet",
        "title": "",
        "width": 4,
        "file": "edit-tweet.html",
        "dialog": true
      }
    ]
  },
  "dependencies": {
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-dropdown-menu": "^3.2.0",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-slider": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "twitter-api-client": "^1.3.7",
    "twitter-media": "^0.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "rollup": "^2.45.2"
  }
}
